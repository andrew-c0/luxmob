    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cumparator ){",
        "\treturn {",
        "\t\t//map cumparator variable to expected task contract input",
        "\t\tcumparatorInput: {",
        "\t\t\tnume: $data.cumparator.nume !== undefined ? $data.cumparator.nume : null,",
        "\t\t\tprenume: $data.cumparator.prenume !== undefined ? $data.cumparator.prenume : null,",
        "\t\t\tadresa: $data.cumparator.adresa !== undefined ? $data.cumparator.adresa : null,",
        "\t\t\ttelefon: $data.cumparator.telefon !== undefined ? $data.cumparator.telefon : null,",
        "\t\t\tparola: $data.cumparator.parola !== undefined ? $data.cumparator.parola : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
